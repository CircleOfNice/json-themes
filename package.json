{
  "name": "json-themes",
  "version": "0.1.0",
  "description": "A toolbox around theming for component libraries",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "dist/module.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "test": "parcel src/test/index.html",
    "lint": "eslint",
    "check": "tsc --noEmit",
    "start": "vite serve src/app",
    "build": "tsc --build --dry && vite build && tsc",
    "pub": "npm run lint && npm run check && npm run build && np"
  },
  "keywords": [
    "react",
    "utils",
    "javascript",
    "typescript",
    "library",
    "component library",
    "theming",
    "react theming"
  ],
  "author": "max-crcl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "workspaces": {
    "packages": [
      "src/lib/*",
      "src/app/*"
    ]
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@babel/core": "^7.17.9",
    "@types/babel__core": "^7.1.19",
    "@types/react": "^17.0.44",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "np": "^7.6.1",
    "react": "^17.0.2",
    "typescript": "^4.6.4",
    "vite": "^2.9.6"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^1.3.1",
    "deepmerge-ts": "^4.0.3",
    "goober": "^2.1.9"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "browserslist": [
    "defaults"
  ],
  "engines": {
    "node": "^12.20.0 || >=14.13.1"
  },
  "staticFiles": {
    "staticPath": "src/static"
  }
}
